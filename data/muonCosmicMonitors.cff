#Analyzer taken from online dqm
include "DQM/TrackingMonitor/data/MonitorTrackSTACosmicMuons.cff"
include "DQM/TrackingMonitor/data/MonitorTrackSTACosmicMuonsHLT.cff"
include "DQM/TrackingMonitor/data/MonitorTrackGLBCosmicMuons.cfi"
include "DQM/TrackingMonitor/data/MonitorTrackTKCosmicMuons.cfi"
include "DQM/DTMonitorModule/data/dtSegmTask.cfi"

sequence muonTrackCosmicAnalyzers = {standAloneCosmicMuonsMonitors, 
    MonitorTrackTKCosmicMuons, 
    MonitorTrackGLBCosmicMuons}

sequence muonTrackCosmicAnalyzersHLT = { MonitorTrackSTACosmicMuonsHLTDT, MonitorTrackSTACosmicMuonsHLTCSC }

#dedicated analyzers for offline dqm
include "RecoMuon/TrackingTools/data/MuonServiceProxy.cff" 
include "DQMOffline/Muon/data/muonCosmicAnalyzer.cfi"
include "DQMOffline/Muon/data/CSCMonitor.cfi"

sequence muonCosmicMonitors = {muonTrackCosmicAnalyzers, dtSegmentsMonitor, cscMonitor, muonCosmicMonitors}
sequence muonCosmicMonitors_woCSC = {muonTrackAnalyzers, dtSegmentsMonitor, muonCosmicMonitors}

module muonStandAloneCosmicAnalyzer = muonAnalyzer from "DQMOffline/Muon/data/muonAnalyzer.cfi"
replace muonStandAloneCosmicAnalyzer.DoMuonRecoAnalysis = false
sequence muonStandAloneCosmicMonitors = {MonitorTrackSTACosmicMuons, dtSegmentsMonitor, cscMonitor, muonStandAloneCosmicAnalyzer}

#dedicated clients for offline dqm 
include "DQMOffline/Muon/data/muonQualityTests.cff"

sequence muonCosmicMonitorsAndQualityTests = {muonCosmicMonitors,muonQualityTests}
